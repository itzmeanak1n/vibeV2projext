{
  "http": {
    "cache": {
      "enabled": true,
      "max_age_short": 300,
      "max_age_long": 86400,
      "static": {
        "enabled": true,
        "max_age": 31536000
      }
    },
    "cors": {
      "enabled": true,
      "allowed_origins": ["*"],
      "allowed_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowed_headers": ["Content-Type", "Authorization"],
      "exposed_headers": [],
      "max_age_seconds": 86400,
      "allow_credentials": true
    }
  }
}
